{
    "name": "technote/laravel-reservation-management-system",
    "type": "project",
    "license": "GPL-2.0+",
    "description": "Reservation management system",
    "homepage": "https://github.com/technote-space/laravel-reservation-management-system",
    "keywords": [
        "reservation"
    ],
    "authors": [
        {
            "name": "Technote",
            "email": "technote.space@gmail.com",
            "homepage": "https://technote.space"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "staudenmeir/eloquent-eager-limit": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-query-detector": "^1.0",
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.3",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 1200
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "if [[ -n $(php artisan list --raw | grep 'ide-helper:generate') ]]; then composer ide; fi",
            "php artisan optimize --ansi",
            "php artisan cache:clear --ansi",
            "php artisan config:clear --ansi",
            "php artisan route:clear --ansi"
        ],
        "setup": [
            "composer install --no-interaction --prefer-dist --no-suggest",
            "@build:js"
        ],
        "build": [
            "@build:php",
            "@build:js"
        ],
        "build:php": [
            "composer install --no-dev --no-interaction --prefer-dist --no-suggest"
        ],
        "build:js": [
            "yarn install",
            "yarn prod"
        ],
        "test": [
            "@stage-lint",
            "@stage-test"
        ],
        "stage-lint": [
            "@bin:phpcs",
            "@bin:phpmd",
            "@bin:eslint",
            "@bin:stylelint"
        ],
        "stage-test": [
            "@bin:jest",
            "@bin:phpunit",
            "@bin:dusk"
        ],
        "test-php": [
            "@bin:phpcs",
            "@bin:phpmd",
            "@bin:phpunit",
            "@bin:dusk"
        ],
        "test-js": [
            "@bin:eslint",
            "@bin:jest"
        ],
        "test-css": [
            "@bin:stylelint"
        ],
        "phpcs": [
            "phpcs --standard=./phpcs.xml"
        ],
        "phpmd": [
            "phpmd ./app/,./config,./database/factories,./database/seeds,./routes/,./tests/ text phpmd.xml"
        ],
        "phpunit": [
            "phpunit --stop-on-failure --colors=always"
        ],
        "phpunit:no": [
            "@phpunit --no-coverage"
        ],
        "phpunit:feature": [
            "@phpunit:no --group=Feature"
        ],
        "phpunit:api": [
            "@phpunit:no --group=Feature.Api"
        ],
        "phpunit:crud": [
            "@phpunit:no --group=Feature.Api.Crud"
        ],
        "phpunit:auth": [
            "@phpunit:no --group=Feature.Api.Auth"
        ],
        "phpunit:unit": [
            "@phpunit:no --group=Unit"
        ],
        "phpunit:models": [
            "@phpunit:no --group=Unit.Models"
        ],
        "phpunit:helpers": [
            "@phpunit:no --group=Unit.Helpers"
        ],
        "dusk": [
            "php artisan dusk --env=testing"
        ],
        "eslint": [
            "yarn lint"
        ],
        "jest": [
            "yarn cover"
        ],
        "jest:update": [
            "yarn test:update"
        ],
        "stylelint": [
            "yarn stylelint"
        ],
        "bin:phpcs": [
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) bash bin/check/phpcs.sh"
        ],
        "bin:phpmd": [
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) bash bin/check/phpmd.sh"
        ],
        "bin:eslint": [
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) bash bin/check/eslint.sh"
        ],
        "bin:stylelint": [
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) bash bin/check/stylelint.sh"
        ],
        "bin:jest": [
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) bash bin/test/jest.sh"
        ],
        "bin:phpunit": [
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) bash bin/test/phpunit.sh"
        ],
        "bin:dusk": [
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) bash bin/test/dusk.sh"
        ],
        "coveralls-php": [
            "php-coveralls -v"
        ],
        "coveralls-js": [
            "yarn coveralls"
        ],
        "migrate": [
            "php artisan migrate:refresh --ansi"
        ],
        "seed": [
            "php artisan config:clear --ansi",
            "php artisan db:seed --ansi"
        ],
        "da": [
            "composer dump-autoload"
        ],
        "ide": [
            "php artisan ide-helper:generate --ansi",
            "php artisan ide-helper:models --write --ansi",
            "php artisan ide-helper:meta --ansi"
        ],
        "packages": [
            "ncu -u",
            "yarn --audit install",
            "composer update"
        ],
        "gh-releases": [
            "@build",
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) source bin/deploy/env.sh && bash bin/deploy/gh-releases.sh"
        ],
        "routes": [
            "php artisan route:clear --ansi",
            "php artisan route:list --ansi"
        ],
        "chrome": [
            "TRAVIS_BUILD_DIR=$(cd $(dirname $0); pwd) bash bin/prepare/install-chrome.sh"
        ]
    }
}
